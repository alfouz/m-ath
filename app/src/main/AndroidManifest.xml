<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.alfouz.tfm.tfm">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity android:name=".AuthenticationActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CourseActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity android:name=".LessonActivity"
            android:parentActivityName=".CourseActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".CourseActivity" />
        </activity>
        <activity android:name=".NewCourseActivity"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity android:name=".EditCourseLessonsActivity"
            android:parentActivityName=".EditCourseActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".EditCourseActivity" />
        </activity>
        <activity android:name=".EditCourseActivity"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity android:name=".NewLessonActivity"
            android:parentActivityName=".EditCourseLessonsActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".EditCourseLessonsActivity" />
        </activity>
        <activity android:name=".MathTaskListActivity"
            android:parentActivityName=".EditLessonActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".EditLessonActivity" />
        </activity>
        <activity android:name=".EditLessonActivity"
            android:parentActivityName=".EditCourseLessonsActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".EditCourseLessonsActivity" />
        </activity>
        <activity android:name=".MathTaskNewActivity"
            android:parentActivityName=".MathTaskListActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MathTaskListActivity" />
        </activity>
    </application>

</manifest>